{
  "name": "@alvarosabu/utils",
  "version": "2.2.0",
  "description": "Personal collection of common JavaScript / TypeScript utils",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev> (https://github.com/alvarosabu/)",
  "files": [
    "dist"
  ],
  "keywords": [
    "utils",
    "javascript",
    "typescript"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/as-utils.umd.cjs",
  "module": "./dist/as-utils.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/as-utils.js",
      "require": "./dist/as-utils.umd.cjs"
    },
    "./*": "./dist/as-utils.es.js"
  },
  "homepage": "https://github.com/alvarosabu/portfolio#readme",
  "repository": "https://github.com/alvarosabu/as-portfolio",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "pathe": "^1.1.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-dts": "^1.7.2",
    "vitest": "^0.28.4"
  }
}
